@prefix : <http://www.example.com/Motology#> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/#> .
@prefix moto: <http://www.example.com/Motology#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://www.example.com/Motology#> .

<http://www.example.com/Motology> rdf:type owl:Ontology ;
                                   dcterms:author "Andrea Serafini" ,
                                                  "Piero Sanchi" ;
                                   rdfs:comment "Un'ontologia sviluppata per modellare il mondo delle competizioni motociclistiche, le informazioni principali e le statistiche di contorno."@it .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/author
dcterms:author rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.example.com/Motology#circuitoSponsorizzatoDa
moto:circuitoSponsorizzatoDa rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf moto:haSponsor ;
                             owl:inverseOf moto:sponsorizzaCircuito ;
                             rdfs:domain moto:Circuito ;
                             rdfs:range moto:Sponsor ;
                             rdfs:comment "Indica che un circuito è sponsorizzato da uno sponsor."@it ;
                             rdfs:label "circuitoSponsorizzatoDa"@it .


###  http://www.example.com/Motology#cittàNataleDi
moto:cittàNataleDi rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf moto:haPilota ;
                   owl:inverseOf moto:natoInCittà ;
                   rdfs:domain moto:Città ;
                   rdfs:range moto:Pilota ;
                   rdfs:comment "Indica la città natale di un pilota."@it ;
                   rdfs:label "cittàNataleDi"@it .


###  http://www.example.com/Motology#compostoDaGP
moto:compostoDaGP rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf moto:haGP ;
                  owl:inverseOf moto:svoltoInCampionato ;
                  rdfs:domain moto:Campionato ;
                  rdfs:range moto:GranPremio ;
                  rdfs:comment "Indica che un campionato è composto da gran premi."@it ;
                  rdfs:label "compostoDaGP"@it .


###  http://www.example.com/Motology#corsaInGP
moto:corsaInGP rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf moto:haGP ;
               rdfs:domain moto:Competizione ;
               rdfs:range moto:GranPremio ;
               rdfs:comment "Indica che una competizione si svolge in un gran premio."@it ;
               rdfs:label "corsaInGP"@it .


###  http://www.example.com/Motology#costruitaDa
moto:costruitaDa rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf moto:haCostruttore ;
                 owl:inverseOf moto:produttoreDi ;
                 rdfs:domain moto:Moto ;
                 rdfs:range moto:Costruttore ;
                 rdfs:comment "Indica che una moto è costruita da un costruttore."@it ;
                 rdfs:label "costruitaDa"@it .


###  http://www.example.com/Motology#dipendenteDelTeam
moto:dipendenteDelTeam rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf moto:haTeam ;
                       owl:inverseOf moto:haStaff ;
                       rdfs:domain moto:TeamStaff ;
                       rdfs:range moto:Team ;
                       rdfs:comment "Indica che un membro dello staff dipende da un team."@it ;
                       rdfs:label "dipendenteDelTeam"@it .


###  http://www.example.com/Motology#guidaMoto
moto:guidaMoto rdf:type owl:ObjectProperty ;
               owl:inverseOf moto:guidataDa ;
               rdfs:domain moto:Pilota ;
               rdfs:range moto:Moto ;
               rdfs:comment "Indica che un pilota guida una moto."@it ;
               rdfs:label "guidaMoto"@it .


###  http://www.example.com/Motology#guidataDa
moto:guidataDa rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf moto:haPilota ;
               rdfs:domain moto:Moto ;
               rdfs:range moto:Pilota ;
               rdfs:comment "Indica che una moto è guidata da un pilota."@it ;
               rdfs:label "guidataDa"@it .


###  http://www.example.com/Motology#haCapotecnico
moto:haCapotecnico rdf:type owl:ObjectProperty ;
                   owl:inverseOf moto:seguePilota ;
                   rdfs:domain moto:Pilota ;
                   rdfs:range moto:Capotecnico ;
                   rdfs:comment "Indica che un pilota ha un capotecnico."@it ;
                   rdfs:label "haCapotecnico"@it .


###  http://www.example.com/Motology#haCircuito
moto:haCircuito rdf:type owl:ObjectProperty ;
                rdfs:range moto:Circuito ;
                rdfs:comment "Indica che un oggetto ha un circuito associato."@it ;
                rdfs:label "haCircuito"@it .


###  http://www.example.com/Motology#haCittà
moto:haCittà rdf:type owl:ObjectProperty ;
             rdfs:range moto:Città ;
             rdfs:comment "Indica che un oggetto ha una città associata."@it ;
             rdfs:label "haCittà"@it .


###  http://www.example.com/Motology#haCostruttore
moto:haCostruttore rdf:type owl:ObjectProperty ;
                   rdfs:range moto:Costruttore ;
                   rdfs:comment "Indica che un oggetto ha un costruttore associato."@it ;
                   rdfs:label "haCostruttore"@it .


###  http://www.example.com/Motology#haCostruttoreDelTeam
moto:haCostruttoreDelTeam rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf moto:haCostruttore ;
                          rdfs:domain moto:Team ;
                          rdfs:range moto:Costruttore ;
                          rdfs:comment "Indica che un team utilizza moto di un costruttore." ;
                          rdfs:label "haCostruttoreDelTeam" .


###  http://www.example.com/Motology#haGP
moto:haGP rdf:type owl:ObjectProperty ;
          rdfs:range moto:GranPremio ;
          rdfs:comment "Indica che un oggetto ha un gran premio associato."@it ;
          rdfs:label "haGP"@it .


###  http://www.example.com/Motology#haInClassifica
moto:haInClassifica rdf:type owl:ObjectProperty ;
                    owl:inverseOf moto:inClassificaDi ;
                    rdfs:domain moto:Competizione ;
                    rdfs:range moto:Posizionamento ;
                    rdfs:comment "Indica che una competizione ha un piazzamento in classifica." ;
                    rdfs:label "haInCassifica" .


###  http://www.example.com/Motology#haInScuderia
moto:haInScuderia rdf:type owl:ObjectProperty ;
                  owl:inverseOf moto:motoDelTeam ;
                  rdfs:domain moto:Team ;
                  rdfs:range moto:Moto ;
                  rdfs:comment "Indica che un team ha una moto nella propria scuderia."@it ;
                  rdfs:label "haInScuderia"@it .


###  http://www.example.com/Motology#haMotore
moto:haMotore rdf:type owl:ObjectProperty ;
              owl:inverseOf moto:montatoDa ;
              rdfs:domain moto:Moto ;
              rdfs:range moto:Motore ;
              rdfs:comment "Indica che una moto ha un motore associato."@it ;
              rdfs:label "haMotore"@it .


###  http://www.example.com/Motology#haNazione
moto:haNazione rdf:type owl:ObjectProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( moto:Circuito
                                           moto:Città
                                           moto:Costruttore
                                           moto:GranPremio
                                           <http://xmlns.com/foaf/0.1/Person>
                                         )
                           ] ;
               rdfs:range moto:Nazione ;
               owl:propertyChainAxiom ( moto:haCittà
                                        moto:haNazione
                                      ) ,
                                      ( moto:svoltoPresso
                                        moto:situatoInCittà
                                        moto:haNazione
                                      ) ;
               rdfs:comment "Indica che un oggetto proviene da o è associata ad una nazione."@it ;
               rdfs:label "haNazione"@it .


###  http://www.example.com/Motology#haOttenuto
moto:haOttenuto rdf:type owl:ObjectProperty ;
                owl:inverseOf moto:piazzamentoDi ;
                rdfs:domain moto:Pilota ;
                rdfs:range moto:Posizionamento ;
                rdfs:comment "Indica che un pilota ha ottenuto un piazzamento."@it ;
                rdfs:label "haOttenuto"@it .


###  http://www.example.com/Motology#haPilota
moto:haPilota rdf:type owl:ObjectProperty ;
              rdfs:range moto:Pilota ;
              rdfs:comment "Indica che un oggetto è associato ad un pilota."@it ;
              rdfs:label "haPilota"@it .


###  http://www.example.com/Motology#haSponsor
moto:haSponsor rdf:type owl:ObjectProperty ;
               rdfs:range moto:Sponsor ;
               rdfs:comment "Indica che un oggetto è associato ad uno sponsor."@it ;
               rdfs:label "haSponsor"@it .


###  http://www.example.com/Motology#haStaff
moto:haStaff rdf:type owl:ObjectProperty ;
             rdfs:domain moto:Team ;
             rdfs:range moto:TeamStaff ;
             rdfs:comment "Indica che un team ha uno staff."@it ;
             rdfs:label "haStaff"@it .


###  http://www.example.com/Motology#haTeam
moto:haTeam rdf:type owl:ObjectProperty ;
            rdfs:range moto:Team ;
            rdfs:comment "Indica che ad un oggetto è associato un team."@it ;
            rdfs:label "haTeam"@it .


###  http://www.example.com/Motology#inClassificaDi
moto:inClassificaDi rdf:type owl:ObjectProperty ;
                    rdfs:domain moto:Posizionamento ;
                    rdfs:range moto:Competizione ;
                    rdfs:comment "Indica che il piazzamento concorre alla classifica di una competizione."@it ;
                    rdfs:label "inClassificaDi"@it .


###  http://www.example.com/Motology#lavoraPer
moto:lavoraPer rdf:type owl:ObjectProperty ;
               rdfs:domain moto:Stuart ;
               rdfs:range moto:Circuito ;
               rdfs:comment "Indica che uno stuart lavora per un circuito."@it ;
               rdfs:label "lavoraPer"@it .


###  http://www.example.com/Motology#montatoDa
moto:montatoDa rdf:type owl:ObjectProperty ;
               rdfs:domain moto:Motore ;
               rdfs:range moto:Moto ;
               rdfs:comment "Indica che un motore è montato su una moto."@it ;
               rdfs:label "montatoDa"@it .


###  http://www.example.com/Motology#motoDelTeam
moto:motoDelTeam rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf moto:haTeam ;
                 rdfs:domain moto:Moto ;
                 rdfs:range moto:Team ;
                 rdfs:comment "Indica che una moto appartiene ad un team."@it ;
                 rdfs:label "motoDelTeam"@it .


###  http://www.example.com/Motology#natoInCittà
moto:natoInCittà rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf moto:haCittà ;
                 rdfs:domain moto:Pilota ;
                 rdfs:range moto:Città ;
                 rdfs:comment "Indica che un pilota è nato in una città."@it ;
                 rdfs:label "natoInCittà"@it .


###  http://www.example.com/Motology#ospitaCircuito
moto:ospitaCircuito rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf moto:haCircuito ;
                    owl:inverseOf moto:situatoInCittà ;
                    rdfs:domain moto:Città ;
                    rdfs:range moto:Circuito ;
                    rdfs:comment "Indica che una città ospita un circuito."@it ;
                    rdfs:label "ospitaCircuito"@it .


###  http://www.example.com/Motology#ospitaGP
moto:ospitaGP rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf moto:haGP ;
              owl:inverseOf moto:svoltoPresso ;
              rdfs:domain moto:Circuito ;
              rdfs:range moto:GranPremio ;
              rdfs:comment "Indica che un circuito ospita un gran premio."@it ;
              rdfs:label "ospitaGP"@it .


###  http://www.example.com/Motology#piazzamentoDi
moto:piazzamentoDi rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf moto:haPilota ;
                   rdfs:domain moto:Posizionamento ;
                   rdfs:range moto:Pilota ;
                   rdfs:comment "Indica che un piazzamento è stato ottenuto da un pilota."@it ;
                   rdfs:label "piazzamentoDi"@it .


###  http://www.example.com/Motology#pilotaDelTeam
moto:pilotaDelTeam rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf moto:haTeam ;
                   owl:inverseOf moto:teamDi ;
                   rdfs:domain moto:Pilota ;
                   rdfs:range moto:Team ;
                   rdfs:comment "Indica che un pilota corre per un team."@it ;
                   rdfs:label "pilotaDelTeam"@it .


###  http://www.example.com/Motology#posizionamentoDiCostruttore
moto:posizionamentoDiCostruttore rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf moto:haCostruttore ;
                                 rdfs:domain moto:PosizionamentoCampionato ;
                                 rdfs:range moto:Costruttore ;
                                 rdfs:comment "Indica che il costruttore ha ottenuto un posizionamento."@it ;
                                 rdfs:label "posizionamentoDiCostruttore"@it .


###  http://www.example.com/Motology#produttoreDi
moto:produttoreDi rdf:type owl:ObjectProperty ;
                  rdfs:domain moto:Costruttore ;
                  rdfs:range moto:Moto ;
                  rdfs:comment "Indica che un produttore ha costruito una moto."@it ;
                  rdfs:label "produttoreDi"@it .


###  http://www.example.com/Motology#proveInGP
moto:proveInGP rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf moto:haGP ;
               rdfs:domain moto:ProveLibere ;
               rdfs:range moto:GranPremio ;
               rdfs:comment "Indica che un turno di prove libere si svolge in un gran premio."@it ;
               rdfs:label "proveInGP"@it .


###  http://www.example.com/Motology#qualificatoConTempo
moto:qualificatoConTempo rdf:type owl:ObjectProperty ;
                         owl:inverseOf moto:tempoInQ ;
                         rdfs:domain moto:PosizionamentoQualifiche ;
                         rdfs:range moto:MigliorTempo ;
                         rdfs:comment "Indica che un posizionamento in qualifica ha un tempo associato."@it ;
                         rdfs:label "qualificatoConTempo"@it .


###  http://www.example.com/Motology#seguePilota
moto:seguePilota rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf moto:haPilota ;
                 rdfs:domain moto:Capotecnico ;
                 rdfs:range moto:Pilota ;
                 rdfs:comment "Indica che un capotecnico segue un pilota."@it ;
                 rdfs:label "seguePilota"@it .


###  http://www.example.com/Motology#situatoInCittà
moto:situatoInCittà rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf moto:haCittà ;
                    rdfs:domain moto:Circuito ;
                    rdfs:range moto:Città ;
                    rdfs:comment "Indica che un circuito è situato in una città."@it ;
                    rdfs:label "situatoInCittà"@it .


###  http://www.example.com/Motology#sponsorizzaCircuito
moto:sponsorizzaCircuito rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf moto:haCircuito ;
                         rdfs:domain moto:Sponsor ;
                         rdfs:range moto:Circuito ;
                         rdfs:comment "Indica che uno sponsor sponsorizza un circuito."@it ;
                         rdfs:label "sponsorizzaCircuito"@it .


###  http://www.example.com/Motology#sponsorizzaTeam
moto:sponsorizzaTeam rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf moto:haTeam ;
                     owl:inverseOf moto:teamSponsorizzatoDa ;
                     rdfs:domain moto:Sponsor ;
                     rdfs:range moto:Team ;
                     rdfs:comment "Indica che un sponsor sponsorizza un team."@it ;
                     rdfs:label "sponsorizzaTeam"@it .


###  http://www.example.com/Motology#svoltoInCampionato
moto:svoltoInCampionato rdf:type owl:ObjectProperty ;
                        rdfs:domain moto:GranPremio ;
                        rdfs:range moto:Campionato ;
                        rdfs:comment "Indica che un gran premio è associato ad un campionato."@it ;
                        rdfs:label "svoltoInCampionato"@it .


###  http://www.example.com/Motology#svoltoPresso
moto:svoltoPresso rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf moto:haCircuito ;
                  rdfs:domain moto:GranPremio ;
                  rdfs:range moto:Circuito ;
                  rdfs:comment "Indica che un gran premio si svolge presso un circuito."@it ;
                  rdfs:label "svoltoPresso"@it .


###  http://www.example.com/Motology#teamDi
moto:teamDi rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf moto:haPilota ;
            rdfs:domain moto:Team ;
            rdfs:range moto:Pilota ;
            rdfs:comment "Indica che un team ha un pilota a contratto."@it ;
            rdfs:label "teamDi"@it .


###  http://www.example.com/Motology#teamSponsorizzatoDa
moto:teamSponsorizzatoDa rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf moto:haSponsor ;
                         rdfs:domain moto:Team ;
                         rdfs:range moto:Sponsor ;
                         rdfs:comment "Indica che un team è sponsorizzato da uno sponsor."@it ;
                         rdfs:label "teamSponsorizzatoDa"@it .


###  http://www.example.com/Motology#tempoDi
moto:tempoDi rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf moto:haPilota ;
             rdfs:domain moto:MigliorTempo ;
             rdfs:range moto:Pilota ;
             rdfs:comment "Indica che un tempo è stato segnato da un pilota."@it ;
             rdfs:label "tempoDi"@it .


###  http://www.example.com/Motology#tempoInFP
moto:tempoInFP rdf:type owl:ObjectProperty ;
               rdfs:domain moto:MigliorTempo ;
               rdfs:range moto:ProveLibere ;
               rdfs:comment "Indica che un tempo è associato ad una prova libera."@it ;
               rdfs:label "tempoInFP"@it .


###  http://www.example.com/Motology#tempoInQ
moto:tempoInQ rdf:type owl:ObjectProperty ;
              rdfs:domain moto:MigliorTempo ;
              rdfs:range moto:PosizionamentoQualifiche ;
              rdfs:comment "Indica che un tempo è associato ad un posizionamento in qualifica."@it ;
              rdfs:label "tempoInQ"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.example.com/Motology#haAnno
moto:haAnno rdf:type owl:DatatypeProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( moto:Campionato
                                        moto:GranPremio
                                      )
                        ] ;
            rdfs:range xsd:integer ;
            rdfs:comment "Indica in che anno si svolge un campionato."@it ;
            rdfs:label "haAnno"@it .


###  http://www.example.com/Motology#haCilindrata
moto:haCilindrata rdf:type owl:DatatypeProperty ;
                  rdfs:domain moto:MotoreTermico ;
                  rdfs:range xsd:integer ;
                  rdfs:comment "Indica la cilindrata di un motore termico."@it ;
                  rdfs:label "haCilindrata"@it .


###  http://www.example.com/Motology#haCilindri
moto:haCilindri rdf:type owl:DatatypeProperty ;
                rdfs:domain moto:MotoreTermico ;
                rdfs:range xsd:integer ;
                rdfs:comment "Indica il numero di cilindri di un motore termico."@it ;
                rdfs:label "haCilindri"@it .


###  http://www.example.com/Motology#haCoppia
moto:haCoppia rdf:type owl:DatatypeProperty ;
              rdfs:domain moto:Motore ;
              rdfs:range xsd:integer ;
              rdfs:comment "Indica la coppia di un motore."@it ;
              rdfs:label "haCoppia"@it .


###  http://www.example.com/Motology#haGap
moto:haGap rdf:type owl:DatatypeProperty ;
           rdfs:domain moto:PosizionamentoGara ;
           rdfs:range xsd:string ;
           rdfs:comment "Indica il gap nel posizionamento in gara."@it ;
           rdfs:label "haGap"@it .


###  http://www.example.com/Motology#haLunghezza
moto:haLunghezza rdf:type owl:DatatypeProperty ;
                 rdfs:domain moto:Circuito ;
                 rdfs:range xsd:integer ;
                 rdfs:comment "Indica la lunghezza di un circuito."@it ;
                 rdfs:label "haLunghezza"@it .


###  http://www.example.com/Motology#haNome
moto:haNome rdf:type owl:DatatypeProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( moto:Costruttore
                                        moto:Sponsor
                                      )
                        ] ;
            rdfs:range xsd:string ;
            rdfs:comment "Indica il nome di un oggetto."@it ;
            rdfs:label "haNome"@it .


###  http://www.example.com/Motology#haNomePersona
moto:haNomePersona rdf:type owl:DatatypeProperty ;
                   rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Indica il nome proprio di una persona."@it ;
                   rdfs:label "haNomePersona"@it .


###  http://www.example.com/Motology#haNumero
moto:haNumero rdf:type owl:DatatypeProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( moto:Moto
                                          moto:Pilota
                                        )
                          ] ;
              rdfs:range xsd:integer ;
              rdfs:comment "Indica il numero di un pilota."@it ;
              rdfs:label "haNumero"@it .


###  http://www.example.com/Motology#haPosizioneFinale
moto:haPosizioneFinale rdf:type owl:DatatypeProperty ;
                       rdfs:domain moto:Posizionamento ;
                       rdfs:range xsd:integer ;
                       rdfs:comment "Indica la posizione del piazzamento."@it ;
                       rdfs:label "haPosizioneFinale"@it .


###  http://www.example.com/Motology#haPotenza
moto:haPotenza rdf:type owl:DatatypeProperty ;
               rdfs:domain moto:Motore ;
               rdfs:range xsd:integer ;
               rdfs:comment "Indica la potenza di un motore."@it ;
               rdfs:label "haPotenza"@it .


###  http://www.example.com/Motology#haPunti
moto:haPunti rdf:type owl:DatatypeProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( moto:PosizionamentoCampionato
                                         moto:PosizionamentoGara
                                       )
                         ] ;
             rdfs:range xsd:integer ;
             rdfs:comment "Indica i punti ottenuti dal piazzamento."@it ;
             rdfs:label "haPunti"@it .


###  http://www.example.com/Motology#haTempo
moto:haTempo rdf:type owl:DatatypeProperty ;
             rdfs:domain moto:MigliorTempo ;
             rdfs:range xsd:string ;
             rdfs:comment "Indica il miglior tempo registrato."@it ;
             rdfs:label "haTempo"@it .


###  http://www.example.com/Motology#haTracciato
moto:haTracciato rdf:type owl:DatatypeProperty ;
                 rdfs:domain moto:Circuito ;
                 rdfs:range xsd:anyURI ;
                 rdfs:comment "Indica il collegamento all'immagine del tracciato."@it ;
                 rdfs:label "haTracciato"@it .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/city
dbo:city rdf:type owl:Class ;
         owl:equivalentClass moto:Città ;
